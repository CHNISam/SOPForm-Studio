{
  "name": "sopform-studio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "tsx watch server/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "preact": "^10.27.2",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@types/node": "^24.10.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "tsx": "^4.7.0",
    "concurrently": "^8.2.2"
  }
}
